{
  "name": "@ensdomains/content-hash",
  "version": "2.5.3",
  "description": "simple tool to encode/decode content hash for EIP 1577 compliant ENS Resolvers (fork of pldespaigne/content-hash)",
  "main": "./src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ensdomains/content-hash"
  },
  "scripts": {
    "build": "browserify ./src/index.js --s contentHash > ./dist/index.js",
    "demo": "static .",
    "deploy-demo": "echo \"surge demo https://content-hash.surge.sh\"",
    "pub": "npm run build && npm publish --access public",
    "test": "mocha",
    "lint": "npx eslint ."
  },
  "author": "pldespaigne",
  "license": "ISC",
  "dependencies": {
    "cids": "^0.8.3",
    "multicodec": "^2.1.0",
    "multihashes": "^2.0.0"
  },
  "devDependencies": {
    "browserify": "^16.5.0",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "mocha": "^5.2.0",
    "node-static": "^0.7.11"
  },
  "keywords": [
    "ethereum",
    "ens",
    "eip-1577",
    "resolver",
    "ipfs",
    "swarm",
    "content-hash",
    "content",
    "hash",
    "contenthash",
    "contentHash"
  ]
}
